    "## 3. 大規模問題への拡張とパフォーマンス分析"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def benchmark_optimization_methods(scenario_sizes=[10, 50, 100, 500]):\n",
    "    \"\"\"\n",
    "    異なるシナリオ数での最適化性能を比較\n",
    "    \"\"\"\n",
    "    benchmark_results = []\n",
    "    \n",
    "    for n_scenarios in scenario_sizes:\n",
    "        print(f\"\\nシナリオ数 {n_scenarios} での性能測定...\")\n",
    "        \n",
    "        # シナリオ生成\n",
    "        np.random.seed(42)\n",
    "        wind_scenarios = np.random.normal(50, 10, n_scenarios)\n",
    "        probabilities = np.ones(n_scenarios) / n_scenarios\n",
    "        \n",
    "        # CVaR最適化のベンチマーク\n",
    "        optimizer = CVaROptimizer(wind_scenarios, probabilities)\n",
    "        \n",
    "        start_time = time.time()\n",
    "        cvar_result = optimizer.solve_cvar_optimization()\n",
    "        cvar_time = time.time() - start_time\n",
    "        \n",
    "        start_time = time.time()\n",
    "        ev_result = optimizer.solve_expected_value_optimization()\n",
    "        ev_time = time.time() - start_time\n",
    "        \n",
    "        if cvar_result['status'] == 'optimal' and ev_result['status'] == 'optimal':\n",
    "            benchmark_results.append({\n",
    "                'scenarios': n_scenarios,\n",
    "                'cvar_time': cvar_time,\n",
    "                'cvar_objective': cvar_result['cvar_value'],\n",
    "                'cvar_x': cvar_result['optimal_x'],\n",
    "                'ev_time': ev_time,\n",
    "                'ev_objective': ev_result['expected_cost'],\n",
    "                'ev_x': ev_result['optimal_x']\n",
    "            })\n",
    "            \n",
    "            print(f\"  CVaR最適化: {cvar_time:.3f}秒\")\n",
    "            print(f\"  期待値最適化: {ev_time:.3f}秒\")\n",
    "        else:\n",
    "            print(f\"  最適化失敗\")\n",
    "    \n",
    "    return pd.DataFrame(benchmark_results)\n",
    "\n",
    "# ベンチマーク実行\n",
    "benchmark_df = benchmark_optimization_methods()\n",
    "\n",
    "if not benchmark_df.empty:\n",
    "    print(\"\\n=== パフォーマンス分析結果 ===\")\n",
    "    display(benchmark_df.round(4))\n",
    "    \n",
    "    # 計算時間の可視化\n",
    "    fig, (ax1, ax2) = plt.subplots(1, 2, figsize=(15, 6))\n",
    "    \n",
    "    # 計算時間\n",
    "    ax1.loglog(benchmark_df['scenarios'], benchmark_df['cvar_time'], 'bo-', label='CVaR Optimization', linewidth=2)\n",
    "    ax1.loglog(benchmark_df['scenarios'], benchmark_df['ev_time'], 'ro-', label='Expected Value', linewidth=2)\n",
    "    ax1.set_xlabel('Number of Scenarios')\n",
    "    ax1.set_ylabel('Computation Time [sec]')\n",
    "    ax1.set_title('Scalability Analysis')\n",
    "    ax1.legend()\n",
    "    ax1.grid(True, alpha=0.3)\n",
    "    \n",
    "    # 目的関数値の比較\n",
    "    ax2.plot(benchmark_df['scenarios'], benchmark_df['cvar_objective'], 'bo-', label='CVaR Objective', linewidth=2)\n",
    "    ax2.plot(benchmark_df['scenarios'], benchmark_df['ev_objective'], 'ro-', label='Expected Cost', linewidth=2)\n",
    "    ax2.set_xlabel('Number of Scenarios')\n",
    "    ax2.set_ylabel('Objective Value [$]')\n",
    "    ax2.set_title('Objective Value Convergence')\n",
    "    ax2.legend()\n",
    "    ax2.grid(True, alpha=0.3)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    # 統計分析\n",
    "    if len(benchmark_df) > 1:\n",
    "        time_ratio = benchmark_df['cvar_time'].iloc[-1] / benchmark_df['cvar_time'].iloc[0]\n",
    "        scenario_ratio = benchmark_df['scenarios'].iloc[-1] / benchmark_df['scenarios'].iloc[0]\n",
    "        complexity = np.log(time_ratio) / np.log(scenario_ratio)\n",
    "        \n",
    "        print(f\"\\n計算複雑度分析:\")\n",
    "        print(f\"シナリオ数が{scenario_ratio:.0f}倍になると計算時間は{time_ratio:.1f}倍\")\n",
    "        print(f\"推定計算複雑度: O(n^{complexity:.2f})\")\nelse:\n",
    "    print(\"ベンチマークに失敗しました\")"
   ]
