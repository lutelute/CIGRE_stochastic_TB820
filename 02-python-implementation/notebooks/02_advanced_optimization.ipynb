   "source": [
    "# 二段階確率計画法の実行\n",
    "two_stage_planner = TwoStageStochasticPlanning(scenarios_data)\n",
    "result = two_stage_planner.solve_two_stage_problem()\n",
    "\n",
    "if result['status'] == 'optimal':\n",
    "    print(\"=== 二段階確率計画法結果 ===\")\n",
    "    print(f\"\\n総コスト: ${result['total_cost']:,.0f}\")\n",
    "    print(f\"投資コスト: ${result['investment_cost']:,.0f}\")\n",
    "    print(f\"期待運用コスト: ${result['expected_operating_cost']:,.0f}\")\n",
    "    \n",
    "    print(f\"\\n=== 最適投資戦略 ===\")\n",
    "    investments = result['investments']\n",
    "    for gen_type, units in investments.items():\n",
    "        if units > 0.01:  # 小さな値は除外\n",
    "            capacity = units * two_stage_planner.generator_options[gen_type]['capacity']\n",
    "            cost = units * two_stage_planner.generator_options[gen_type]['investment_cost']\n",
    "            print(f\"{gen_type.title()}: {units:.2f}ユニット ({capacity:.0f}MW, ${cost:,.0f})\")\n",
    "    \n",
    "    # 運用結果の統計\n",
    "    operations_df = pd.DataFrame(result['operations'])\n",
    "    \n",
    "    print(f\"\\n=== 運用統計 ===\")\n",
    "    for gen_type in ['coal', 'gas', 'renewable']:\n",
    "        if gen_type in operations_df.columns:\n",
    "            avg_output = operations_df[gen_type].mean()\n",
    "            max_output = operations_df[gen_type].max()\n",
    "            print(f\"{gen_type.title()}: 平均 {avg_output:.1f}MW, 最大 {max_output:.1f}MW\")\n",
    "    \n",
    "    avg_shortage = operations_df['shortage'].mean()\n",
    "    max_shortage = operations_df['shortage'].max()\n",
    "    shortage_freq = (operations_df['shortage'] > 0.1).mean()\n",
    "    print(f\"不足電力: 平均 {avg_shortage:.1f}MW, 最大 {max_shortage:.1f}MW, 頻度 {shortage_freq:.1%}\")\n",
    "    \n",
    "    # 可視化\n",
    "    fig, ((ax1, ax2), (ax3, ax4)) = plt.subplots(2, 2, figsize=(15, 12))\n",
    "    \n",
    "    # 投資結果\n",
    "    gen_types = list(investments.keys())\n",
    "    investment_units = [investments[gt] for gt in gen_types]\n",
    "    ax1.bar(gen_types, investment_units, alpha=0.7)\n",
    "    ax1.set_ylabel('Investment Units')\n",
    "    ax1.set_title('Optimal Investment Strategy')\n",
    "    ax1.grid(True, alpha=0.3)\n",
    "    \n",
    "    # シナリオ別運用\n",
    "    scenarios = operations_df['scenario']\n",
    "    for gen_type in ['coal', 'gas', 'renewable']:\n",
    "        if gen_type in operations_df.columns:\n",
    "            ax2.plot(scenarios, operations_df[gen_type], 'o-', label=gen_type.title(), alpha=0.7)\n",
    "    ax2.set_xlabel('Scenario')\n",
    "    ax2.set_ylabel('Generation [MW]')\n",
    "    ax2.set_title('Generation by Scenario')\n",
    "    ax2.legend()\n",
    "    ax2.grid(True, alpha=0.3)\n",
    "    \n",
    "    # 需給バランス\n",
    "    scenario_demands = [scenarios_data[s]['demand'] for s in range(n_scenarios)]\n",
    "    scenario_winds = [scenarios_data[s]['wind'] for s in range(n_scenarios)]\n",
    "    total_generation = operations_df[['coal', 'gas', 'renewable']].sum(axis=1)\n",
    "    \n",
    "    ax3.plot(scenarios, scenario_demands, 'r-', label='Demand', linewidth=2)\n",
    "    ax3.plot(scenarios, total_generation + scenario_winds, 'b--', label='Total Supply', linewidth=2)\n",
    "    ax3.fill_between(scenarios, 0, operations_df['shortage'], alpha=0.3, color='red', label='Shortage')\n",
